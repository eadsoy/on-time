
<div id="map"
     style="width: 50%; height: 300px;"
     data-markers=""
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"></div>

<%= simple_form_for @route do |f| %>
    <%= f.input :start, as: :string, input_html: { id: 'search'} %>
    <%= f.input :end, as: :string, input_html: { id: 'search-end'} %>
    <%= f.submit "Send", class: 'btn btn-dark' %>
<% end %>


<a href="" id='new-band'></a>
